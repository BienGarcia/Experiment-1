#include <iostream>
#include<iomanip>
#include <conio.h>

using namespace std;

int main ()
{
	
	double Mass; 
	double Density;
	
	cout << "Volume" << "=" << "Mass" << "/" << "Density" << endl;

	cout << "Mass in grams" << endl;

	cin >> Mass;

	cout << "Density in grams/cubic meter" << endl;

	cin >> Density;

	cout << "Volume" << '	' << "=" <<	'	' ;

    cout << setprecision(3) << Mass / Density << endl;

	cout << endl; 
	
	_getch();
	return 0;
}
